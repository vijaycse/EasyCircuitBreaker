package com.officedepot.services.storelocator.model;

import java.io.Serializable;

public class GeolocationInfo implements Serializable {
  
	private static final long serialVersionUID = 6982396697447336971L;
	private String zip = "";
    private String city = "";
    private String state = "";
    private String country = "";
 
    public GeolocationInfo(String zip, String city, String state, String country){
    	this.zip = (zip == null ? "" :zip);
    	this.city =  (city == null ? "" :city);
    	this.state =  (state == null ? "" :state);
    	this.country =  (country == null ? "" :country);
    	
    }
    
    
    public String getZip() {
		return zip;
	}

	public String getCity() {
		return city;
	}

	public String getState() {
		return state;
	}

	public String getCountry() {
		return country;
	}

    
}   